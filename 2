for _ in range(3):
    for i in range(1, 101):
        print(i)

i = 1
while i <=3:
    num = 1
    i = i + 1
    while num <= 100:
        print(num, end=' ')
        num = num + 1

i = 0
while True:
    num = 0
    if i == 3:
        break
    else:
        while True:
            num = num + 1
            if num == 101:
                i = i + 1
                break
            else:
                print(num)
